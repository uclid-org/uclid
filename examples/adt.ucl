module main {
    datatype list = | cons(head: integer, tail: list) | nil;

    var l : list;

    init {
        l = nil();
    }
    
    next {
        l' = cons(1, l);
    }

    invariant test : l.head == 2 || l == nil();

    control {
        bmc(7);
        check;
        print_results;
    }
}
