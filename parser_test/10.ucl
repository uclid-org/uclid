module cpu {
    type obs_t = record {
        cache       : [bv4]bv4
    };

    var cache       : [bv4]bv4;
    var obs         : obs_t;

    next {
        havoc obs;
    }
}

module main {
    instance cpu1 : cpu();

    next {
        next(cpu1);
    }
	
    invariant trivial : (forall (a : bv4) :: cpu1.obs.cache[a] == 0bv4);

    control {
	
        check;
        print_results;
        v.print_cex_json(cpu1.obs);
    }
}
