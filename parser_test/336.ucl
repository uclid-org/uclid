    ensures ( (o1.1 == o1.3 && o2.1 == o1.2 && o2.2 == o2.3 && res.1 == res.2) ==> (res.3 == res.1)); //transitivity
    ensures ( (o1.1 == o2.2 && o1.2 == o2.1) ==> ((o1.1 == o2.1) ==> ((res.1 == res.2) && (res.1 == 0)) &&
                                                ((o1.1 != o2.1) ==> (res.1 == -res.2)))); //antisymmetric
    
    {
        res = 0;
