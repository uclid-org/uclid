module main
	{ push(); 
  var tail           : integer;
  var initialized    : boolean;

  input push_valid   : boolean;
  output value_found : boolean;

  init {
    tail = 0;
    initialized = false;
  }

  next {
    initialized' = true;

    if (push_valid) {
      call push();
    }
  }

  invariant push_affects_tail: initialized ==> (push_valid ==> (tail == past(tail) + 2));

  procedure push()
    requires push_valid;
    ensures tail == old(tail) + 1;
    modifies tail;
  {
    tail = tail + 1;
  }
	
  control {
    v1 = verify(push);
	v2 = unroll(4);
	check;
	print_results;
	//v2.print_cex(push_valid, : tail);
  }
}

