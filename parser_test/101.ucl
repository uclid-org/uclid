    /* No Hyperproperty, hence shouldn't be translated */
    procedure isGreaterThan(p: integer, q:integer) returns (res: boolean)
    ensures ( (res == true) ==> ( p > q));
    ensures ( (res == false) ==> ( p <= q));
    {
	res=true; /* 
        if( p <= q) 
