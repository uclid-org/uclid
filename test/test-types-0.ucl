module main {
  type t0 = integer;
  type t1 = bool;
  type t2 = enum {red, green, blue};
  type t3a = {t0, t0, t1};
  type t3b = record {a: t0, b: t0, c: t1};
  type t4 = record {d: t1, e: t3b};
  type t5 = record {a: t0, b: t0, c: t4 };

  var y_global: t0;
  var z_global : t0;

  input mem_in : integer -> integer;

  procedure f(x: t0, y: t0) returns (z:t0) { 
    var x_local: t0;
    assume (x_local > 0); 
    assert (x_local <= 1); 
    havoc x_local; 
    case 
        x_local > 0 : { z = x + y; }
        x_local <= 0 : { z = x - y; }
    esac 
  }
  
  output mem_out : [integer] integer;
  
  init {
    havoc y_global;
    havoc z_global;
  }
  
  next {
    call (y_global) = f(y_global, z_global);
    z_global = y_global + 1;  
  }
}
