module main {

    type cache_t = record { valid : boolean, value : integer };

    var cache   : cache_t;
    var cache1  : cache_t;

    init {
        cache1 = cache[value := 2022];
    }

    next {
        cache1' = cache[value := 42];
    }

    invariant trivial1  : cache1.value == 2022 || cache1.value == 42;

    control {
        v = bmc(1);
        check;
        print_results;
        v.print_cex_json(cache1);
        print_module();
    }
}
