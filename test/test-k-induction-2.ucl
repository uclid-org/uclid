module main {
    var x : integer;
    var b : boolean;

    init {
        b = true;
        x = 0;
    }

    next {
        b' = !b;
        if (b) {
            x' = x + 1;
        } else {
            x' = x - 2;
        }
    }

    invariant x_ge_0: x >= 0;

    control {
        v = induction(2);
        check;
        // print_results;
        // v.print_cex;
    }
}
