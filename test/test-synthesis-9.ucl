// run with -e flag to convert enums to bitvectors

module main {
	type enumT1 = enum {t1, t2};
	type enumT2 = enum {t4, t5};
	synthesis function h(x : enumT1, y : enumT1) : boolean;
	var et1 : enumT1;
	var et2 : enumT1;

	init {
		et1 = t1;
		et2 = et1;
	}

	next {
		et2' = et1;
	}

	//invariant strength : (et1 == t1);
	invariant test : (et2 == t1) && h(et1, et2); 

	control {
		v = induction(1);
		check;
	}
}
