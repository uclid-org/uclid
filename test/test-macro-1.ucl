module main {
    var x : integer;
    var y : integer;

    macro one {
        x = 0;
    }

    macro two {
        x = 1;
        y = 1;
    }

    init {
        x = 2;
        y = 2;
        assert(x == 0);
        one;
        assert(x == 0);
        two;
        assert(x == 1);
        assert(y == 1);
    }
    
    control {
        unroll(0);
        check;
        print_results;
    }
}