module test {
  var a, b : integer;

  procedure fib(x: integer, y: integer) returns (z:integer, w:integer) { 
    z, w = y, x + y;
  }
  init {
    a = 0;
    b = 1;
  }
  next {
    call (a, b) = fib(a, b);
  }
  
  control {
  	simulate (5);
  }
}

