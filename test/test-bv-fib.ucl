module main {
  var a, b : bv16;
  procedure fib(x: bv16, y: bv16) returns (z:bv16, w:bv16) { 
    z, w = y, x + y;
  }
  init {
    a' = 0bv16;
    b' = 1bv16;
  }
  next {
    call (a', b') = fib(a, b);
  }
  
  invariant a_le_b : (a <= b);
  invariant a_lt_b : (a < b);
  invariant a_le_b_v2 : (a+b <= b+b);
  invariant a_le_b_v3 : (a-a <= b-a);
  control {
  	x = unroll (5);
  	check;
  	// print_results;
  	// x->print_cex;
  }
}
