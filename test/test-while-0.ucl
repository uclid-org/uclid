module main
{
  var n : integer;

  init {
    n = 0;
  }

  next {
    call (n') = add10(n);
  }

  procedure add10(x : integer)
    returns (y : integer)
    ensures (y == x + 10);
  {
    var i : integer;
    i = 0;
    y = x;
    while (i < 10)
      invariant (y == x + i);
    {
      y = y + 1;
      i = i + 1;
    }
  }

  control {
    print_module;
    v = verify(add10);
    check;
    print_results;
    v.print_cex(x, y, i);
  }
}
