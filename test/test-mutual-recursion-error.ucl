module main {
  var s1, s2 : integer;

  procedure foo(a : integer, b : integer) returns (z : integer) {
    if (a > 0) {
      call (z) = bar(a + b);
    } else {
      z = a + b;
    }
  }

  procedure bar(s : integer) returns (w : integer) {
    var x : integer;
    var y : integer;
    
    call (x, y) = zoom(s + s, 2);
    w = x + y;
  }

  procedure zoom(s1 : integer, s2 : integer) returns (x : integer,  y : integer) {
    call (x) = foo(s1 + s2, s1);
    call (y) = foo(s1, s1 + s2);
  }

  next {
    call (s1) = foo(s1, s2);
  }
}