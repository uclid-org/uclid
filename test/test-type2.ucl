module main {
  type hash_t;
  
  function hash_int(x : integer) : hash_t;
  axiom (forall (x1 : integer, x2 : integer) :: hash_int(x1) != hash_int(x2));
  
  
  input i : integer;
  var v1, v2 : hash_t;
  
  init {
    v1 = hash_int(0);
    v2 = hash_int(1);
  }
  
  next {
    v1' = hash_int(i);
    v2' = hash_int(i+1);
  }
  
  property f_neq : v1 != v2;
  
  control {
    unroll (3);
    check;
    // print_results;
  }
}

    
